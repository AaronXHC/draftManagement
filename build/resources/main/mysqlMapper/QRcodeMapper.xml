<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
    <mapper namespace="net.htjs.neo4j.tax.dao.QRcodeMapper">




<insert id="saveQRcode" parameterType="java.util.Map" >
    INSERT INTO QRCODE_WAREHOUSE (id,title,summary,content,generate_time,qrcode_src,author)
    VALUES (replace(uuid(),'-',''),
    #{title,jdbcType=VARCHAR},
    #{summary,jdbcType=VARCHAR},
    #{content,jdbcType=VARCHAR},
    current_timestamp(),
    #{qrcode_src,jdbcType=VARCHAR},
    #{UserId,jdbcType=VARCHAR})
</insert>

    <select id="selectQrcodeList" parameterType="java.util.Map" resultType="java.util.Map">
          select ID,TITLE,SUMMARY,CONTENT,DATE_FORMAT(generate_time,'%Y-%m-%d %H:%i') GENERATE_TIME,QRCODE_SRC,AUTHOR
          from QRCODE_WAREHOUSE
    </select>



</mapper>